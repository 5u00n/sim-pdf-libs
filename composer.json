{
    "name": "sim-pdf/sim-pdf-libs",
    "description": "Advanced PDF generation library for Laravel with multi-page support, custom page breaks, headers/footers, and comprehensive styling",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Suren",
            "email": "surenhembram97@gmail.com",
            "homepage": "https://github.com/5u00n"
        }
    ],
    "homepage": "https://github.com/5u00n/sim-pdf-libs",
    "keywords": [
        "pdf",
        "laravel",
        "dompdf",
        "multi-page",
        "page-break",
        "headers",
        "footers",
        "styling",
        "watermark",
        "bookmark"
    ],
    "support": {
        "issues": "https://github.com/5u00n/sim-pdf-libs/issues",
        "source": "https://github.com/5u00n/sim-pdf-libs"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "dompdf/dompdf": "^3.0.1",
        "barryvdh/laravel-dompdf": "^3.1.1",
        "intervention/image": "^3.11.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SimPdf\\SimPdfLibs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimPdf\\SimPdfLibs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimPdf\\SimPdfLibs\\SimPdfServiceProvider"
            ],
            "aliases": {
                "SimPdf": "SimPdf\\SimPdfLibs\\Facades\\SimPdf"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
