{
    "name": "sim-pdf/sim-pdf-libs",
    "description": "Advanced PDF generation library for Laravel with multi-page support, custom page breaks, headers/footers, and comprehensive styling",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SIM PDF Team",
            "email": "support@simpdf.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "dompdf/dompdf": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "SimPdf\\SimPdfLibs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimPdf\\SimPdfLibs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimPdf\\SimPdfLibs\\SimPdfServiceProvider"
            ],
            "aliases": {
                "SimPdf": "SimPdf\\SimPdfLibs\\Facades\\SimPdf"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
